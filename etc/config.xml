<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <conversionpro>
            <general_settings>
                <enabled>1</enabled>
                <host>uitemplatev3.celebros.com</host>
                <adddiv>1</adddiv>
                <addscripts>1</addscripts>
                <scriptslocation>body</scriptslocation>
                <hidecontent>1</hidecontent>
            </general_settings>
            <nav_to_search_settings>
                <nav_to_search>1</nav_to_search>
            </nav_to_search_settings>
            <advanced>
                <scripts_path>uitemplate/Scripts</scripts_path>
                <client_config_path>uitemplate/Clients</client_config_path>
                <client_config_js_filename>ClientConfig.js</client_config_js_filename>
                <angular_client_config_path>UITemplateV6/Clients</angular_client_config_path>
                <angular_client_config_js_filename>CelScripts.js</angular_client_config_js_filename>
                <js_framework>jquery</js_framework>
            </advanced>
        </conversionpro>
    </default>
</config>
